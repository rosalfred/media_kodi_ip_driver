<!-- This is a sample launch file, please change it based on your needs -->

<launch>

	<!-- DRIVERS -->
    <node pkg="media_xbmc_driver"
			name="xbmc_salon"
			type="media_xbmc_driver"
			args="com.alfred.ros.xbmc.XbmcNode"
			output="screen">
		<param name="rate" value="1" />
		<param name="mac" value="" />
		<param name="ip" value="192.168.0.10" />
		<param name="tf_prefix" value="xbmc_salon" />
	</node>

</launch>
